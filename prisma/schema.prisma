generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model video {
  id            String   @id @default(uuid())
  nome          String
  path          String
  transcription String?
  createdAt     DateTime @default(now())
}

model prompt {
  id       String @id @default(uuid())
  title    String
  template String
}
